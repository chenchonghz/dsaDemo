<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:headerattr="http://schemas.android.com/apk/res/com.szrjk.dhome"
    android:id="@+id/rl_editdata_doctor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_bg" >

    <com.szrjk.widget.HeaderView
        android:id="@+id/hv_edit_doctor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        headerattr:htextstr="@string/edit_data" >
    </com.szrjk.widget.HeaderView>

    <TextView
        android:id="@+id/tv_submit_doctor"
        android:layout_width="60dip"
        android:layout_height="@dimen/header_height"
        android:layout_alignParentRight="true"
        android:background="@drawable/button_send_selector"
        android:gravity="center"
        android:text="@string/submit"
        android:textColor="@color/white"
        android:textSize="@dimen/font_size_middle" />

    <ScrollView
        android:id="@+id/sv_edit_doctor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@id/hv_edit_doctor"
        android:background="@color/base_bg"
        android:fillViewport="true"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@+id/rl_editall_doctor"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/base_bg"
            android:orientation="vertical" >

            <View
                android:id="@+id/view_top"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_25dp"
                android:background="@color/base_bg" />

            <RelativeLayout
                android:id="@+id/rl_avatar"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="75dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/avatar"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <ImageView
                    android:id="@+id/iv_avatar"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_alignParentRight="true"
                    android:layout_margin="@dimen/margin_10dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/icon_headfailed" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_type"
                android:layout_width="match_parent"
                android:layout_height="@dimen/message_item_height"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/type"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <TextView
                    android:id="@+id/tv_type"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:gravity="center_vertical"
                    android:text="@string/type"
                    android:textColor="@color/font_tabletitle"
                    android:textSize="@dimen/font_size_small" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_name"
                android:layout_width="match_parent"
                android:layout_height="@dimen/message_item_height"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/name"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:gravity="center_vertical"
                    android:text="@string/name"
                    android:textColor="@color/font_tabletitle"
                    android:textSize="@dimen/font_size_small" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_sex"
                android:layout_width="match_parent"
                android:layout_height="@dimen/message_item_height"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/sex"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <TextView
                    android:id="@+id/tv_sex"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:gravity="center_vertical"
                    android:text="@string/sex"
                    android:textColor="@color/font_tabletitle"
                    android:textSize="@dimen/font_size_small" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_birthday"
                android:layout_width="match_parent"
                android:layout_height="@dimen/message_item_height"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/birthday"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <TextView
                    android:id="@+id/tv_birthday"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:gravity="center_vertical"
                    android:text="@string/birthday"
                    android:textColor="@color/font_tabletitle"
                    android:textSize="@dimen/font_size_small" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_city"
                android:layout_width="match_parent"
                android:layout_height="@dimen/message_item_height"
                android:layout_marginTop="@dimen/margin_10dp"
                android:background="@color/white" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/input_bg" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/message_item_margin"
                    android:gravity="center_vertical"
                    android:text="@string/city"
                    android:textColor="@color/font_titleanduname"
                    android:textSize="@dimen/font_size_small" />

                <TextView
                    android:id="@+id/et_city"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:gravity="center_vertical"
                    android:text="@string/city"
                    android:textColor="@color/font_tabletitle"
                    android:textSize="@dimen/font_size_small" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_editdoctors"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/base_bg" >

                <RelativeLayout
                    android:id="@+id/rl_hospital"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_alignParentTop="true"
                    android:background="@color/white" >

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/input_bg" />

                    <TextView
                        android:id="@+id/tv_hospital"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="@dimen/message_item_margin"
                        android:gravity="right|center_vertical"
                        android:text="@string/ehospital"
                        android:textColor="@color/font_titleanduname"
                        android:textSize="@dimen/font_size_small" />

                    <AutoCompleteTextView
                        android:id="@+id/et_hospital"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/margin_10dp"
                        android:layout_toRightOf="@id/tv_hospital"
                        android:background="@null"
                        android:completionThreshold="1"
                        android:dropDownHeight="120dp"
                        android:ems="10"
                        android:maxLength="16"
                        android:focusable="false"
                        android:gravity="right|right|center_vertical"
                        android:scrollbars="none"
                        android:singleLine="true"
                        android:text="@string/hospital"
                        android:textColor="@color/font_tabletitle"
                        android:textSize="@dimen/font_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_dept"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_below="@id/rl_hospital"
                    android:background="@color/white" >

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/input_bg" />

                    <TextView
                        android:id="@+id/tv_dept"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="@dimen/message_item_margin"
                        android:gravity="right|center_vertical"
                        android:text="@string/department"
                        android:textColor="@color/font_titleanduname"
                        android:textSize="@dimen/font_size_small" />

                    <AutoCompleteTextView
                        android:id="@+id/et_dept"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/margin_10dp"
                        android:layout_toRightOf="@id/tv_dept"
                        android:background="@null"
                        android:completionThreshold="1"
                        android:dropDownHeight="120dp"
                        android:ems="10"
                        android:focusable="false"
                        android:gravity="right|center_vertical"
                        android:scrollbars="none"
                        android:singleLine="true"
                        android:text="@string/department"
                        android:textColor="@color/font_tabletitle"
                        android:textSize="@dimen/font_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_ptitle"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/message_item_height"
                    android:layout_below="@id/rl_dept"
                    android:background="@color/white" >

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/input_bg" />

                    <TextView
                        android:id="@+id/tv_ptitle"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="@dimen/message_item_margin"
                        android:gravity="right|center_vertical"
                        android:text="@string/ptitle"
                        android:textColor="@color/font_titleanduname"
                        android:textSize="@dimen/font_size_small" />

                    <AutoCompleteTextView
                        android:id="@+id/et_ptitle"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/message_item_height"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/margin_10dp"
                        android:layout_toRightOf="@id/tv_ptitle"
                        android:background="@null"
                        android:completionThreshold="1"
                        android:dropDownHeight="120dp"
                        android:ems="10"
                        android:focusable="false"
                        android:gravity="right|center_vertical"
                        android:scrollbars="none"
                        android:singleLine="true"
                        android:text="@string/ptitle"
                        android:textColor="@color/font_tabletitle"
                        android:textSize="@dimen/font_size_small" />
                </RelativeLayout>

                <View
                    android:id="@+id/v_doctor"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_10dp"
                    android:layout_alignParentBottom="true"
                    android:layout_below="@id/rl_ptitle"
                    android:background="@color/base_bg" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>